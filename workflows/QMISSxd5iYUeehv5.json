    },
    {
      "parameters": {
        "jsCode": "{\n  // OBTENER DATOS DEL INPUT\n  const editFields = $('var').item.json;\n  \n  // EXTRAER DATOS DEL TURNO\n  const paciente = editFields.nombre;\n  let fechaCompleta = editFields.fecha; // Puede venir como \"2025-08-25T18:00:00\" o \"2025-08-25T18:00\"\n  const hora = editFields.hora;   // \"18:00\"\n  const motivo = editFields.servicio;\n  const telefono = editFields.telefono;\n  \n  // QUITAR SEGUNDOS SI VIENEN EN LA FECHA\n  // Si la fecha tiene el formato con segundos (2025-09-08T14:00:00), lo convertimos a sin segundos\n  if (fechaCompleta.includes(':') && fechaCompleta.split(':').length > 2) {\n    // Encontrar la posici√≥n del √∫ltimo \":\" y cortar hasta ah√≠\n    fechaCompleta = fechaCompleta.substring(0, fechaCompleta.lastIndexOf(':'));\n  }\n  \n  // SEPARAR FECHA Y HORA DEL CAMPO fechaCompleta\n  const [soloFecha, soloHora] = fechaCompleta.split('T'); // [\"2025-08-25\", \"18:00\"]\n  \n  // FORMATEAR FECHA PARA MOSTRAR (DD-MM-YYYY)\n  const [a√±o, mes, dia] = soloFecha.split('-');\n  const fechaFormateada = `${dia}-${mes}-${a√±o}`; // \"25-08-2025\"\n  \n  // USAR fechaCompleta YA PROCESADA (sin segundos)\n  const fechaHoraISO = fechaCompleta; // Ahora siempre ser√° \"2025-08-25T18:00\"\n  \n  // CONSTRUIR MENSAJE DE LISTA\n  const requestBody = {\n    to: telefono,\n    buttonText: \"Confirmar ‚úÖ\",\n    title: \"ü¶∑ Consultorio M√©dico\",\n    description: `*Resumen de su turno*\nüë§ *Paciente:* ${paciente}\nüìÖ *Fecha:* ${fechaFormateada}\nüïê *Hora:* ${hora}\nüè• *Servicio:* ${motivo}\nüìç *Recordatorio: Traer DNI y estudios previos*\nSeleccione una opci√≥n:`,\n    footer: \"Centro M√©dico ‚Ä¢ Confirme su asistencia\",\n    sections: [\n      {\n        title: \"üìã Opciones Disponibles\",\n        rows: [\n          {\n            rowId: `‚úÖ CONFIRMADO - ${paciente} | ${fechaHoraISO} | ${motivo}`,\n            title: \"‚úÖ Confirmar Turno\",\n            description: \"Confirmo mi asistencia en la fecha y hora indicada\"\n          }\n        ]\n      }\n    ]\n  };\n  \n  // RETORNAR RESULTADO\n  return [{ json: requestBody }];\n}"
