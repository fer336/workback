    {
      "parameters": {
        "jsCode": "// Extraer datos de Google Maps\nconst html = $input.first().json.data;\nconst leads = [];\n\n// Regex patterns para Maps\nconst phonePattern = /\\\"\\+?[0-9\\s\\-\\(\\)]+\\\"/g;\nconst namePattern = /\\\"title\\\":\\\"([^\\\"]+)\\\"/g;\n\n// Extraer nombres\nconst names = [...html.matchAll(namePattern)].map(match => match[1]);\n\n// Extraer tel茅fonos\nconst phones = [...html.matchAll(phonePattern)]\n  .map(match => match[0].replace(/\\\"/g, ''))\n  .filter(phone => phone.length > 8);\n\n// Combinar datos\nnames.forEach((name, index) => {\n  if (phones[index]) {\n    leads.push({\n      json: {\n        nombre_negocio: name,\n        telefono: phones[index],\n        fuente: 'Google Maps',\n        ciudad: $node['Configuraci贸n'].json.ciudad\n      }\n    });\n  }\n});\n\nreturn leads;"
      },
